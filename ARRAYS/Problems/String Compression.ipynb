   "source": [
    "## Compress String\n",
    "bakrbakr >> 'b2a2k2r2'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def compress(s):\n",
    "    s=s.replace(\" \",\"\");\n",
    "    output=[];\n",
    "    for i in sorted(set(s)):\n",
    "        output+=i;\n",
    "        output+=str(s.count(i));\n",
    "    return \"\".join(i for i in output);\n",
    "    "
   ]
  },
